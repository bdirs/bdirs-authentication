{
  "name": "bdirs-web-api",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/bdirs/bdirs-web-api.git",
  "author": "muhwezi deo <aggrey256@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node dist/index.js",
    "start:dev": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/index.ts",
    "heroku-prebuild": "yarn install && yarn db:migrate",
    "heroku-postbuild": "yarn clean && yarn tsc",
    "db:migrate": "node_modules/.bin/sequelize db:migrate --migrations-compiled-path src/db/migrations",
    "test": "jest --detectOpenHandles --coverage",
    "clean": "rm -rf dist/",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.16",
    "@types/node": "^12.7.5",
    "@types/nodemailer": "^6.2.1",
    "@types/nodemailer-direct-transport": "^1.0.31",
    "@types/nodemailer-smtp-transport": "^2.7.4",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^3.4.5",
    "coveralls": "^3.0.5",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "sequelize-cli-typescript": "^3.2.0-c",
    "sequelize-typescript": "^1.0.0-beta.3",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@hapi/joi": "^15.1.1",
    "@sentry/node": "^5.5.0",
    "@types/dotenv": "^6.1.1",
    "@types/sequelize": "^4.28.4",
    "bcrypt": "^3.0.6",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.3.0",
    "parse-database-url": "^0.3.0",
    "pg": "^7.12.0",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.1.0",
    "uuid": "^3.3.3",
    "snyk": "^1.231.0"
  },
  "engines": {
    "yarn": "1.x"
  },
  "snyk": true
}
